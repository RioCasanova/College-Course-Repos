{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": "google",
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "linebreak-style": "off",
    "require-jsdoc": "off",
    "max-len": "off",
    "quotes": "off",
    "object-curly-spacing": "off",
    "no-unused-vars": "warn",
    "prefer-const": "warn"
  }
}
